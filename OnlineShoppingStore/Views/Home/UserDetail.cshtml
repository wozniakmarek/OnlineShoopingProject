
@{
    ViewBag.Title = "UserDetail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="card">
    <div class="card-header">
        Login User Detail
    </div>
    <div class="card-body">
        <h5 class="card-title">Hello User @HttpContext.Current.User.Identity.Name </h5>
        <p class="card-text"> Now You are Authorized User You Can Access to Admin Panel.</p>
        <a href="/Admin/Dashboard" class="btn btn-primary">AdminPanel</a>
    </div>
</div>




